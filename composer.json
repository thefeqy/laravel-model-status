{
    "name": "thefeqy/laravel-model-status",
    "description": "A Laravel package to automate adding configurable status columns to models and migrations, and enforce user activity through middleware.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammed Elfeqy",
            "email": "thefeqy@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/console": "^11.0|^12.0",
        "illuminate/routing": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Thefeqy\\ModelStatus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thefeqy\\ModelStatus\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thefeqy\\ModelStatus\\LaravelModelStatusServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0",
        "laravel/pint": "^1.20"
    }
}
